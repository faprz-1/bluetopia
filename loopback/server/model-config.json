{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ACL": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mysqlDev",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Usuario": {
    "dataSource": "mysqlDev",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "profileImage": false,
          "login": true,
          "logout": true,
          "register": true,
          "registerAdminis":true,
          "userWithCredentials": true,
          "changePassword": true,
          "resetPassword": true,
          "setPassword": true,
          "testPush": true,
          "prototype.updatePushToken": true,
          "prototype.changeProfileImage": true
        }
      }
    }
  },
  "UsuarioAccessToken": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Upload": {
    "dataSource": "localFiles",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "download": true
        }
      }
    }
  },
  "UploadedFiles": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Notification": {
    "dataSource": "mysqlDev",
    "public": true
  },
  "PushTokens": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Mail": {
    "dataSource": "mailDev",
    "public": false
  },
  "PasswordResetPIN": {
    "dataSource": "mysqlDev",
    "public": true
  },
  "adminMail": {
    "dataSource": "adminMail",
    "public": false
  }

}
