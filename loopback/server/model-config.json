{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ACL": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mysqlDev",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Usuario": {
    "dataSource": "mysqlDev",
    "public": true,
    "options": {
      "allowEternalTokens": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "profileImage": false,
          "loginIfActive": true,
          "logout": true,
          "register": true,
          "registerAdmins": true,
          "RegisterWithEmailVerification": true,
          "userWithCredentials": true,
          "getAnyUsers": true,
          "prototype.updateAnyUser": true,
          "changeAnyPassword": true,
          "changePassword": true,
          "resetPassword": true,
          "setPassword": true,
          "getRoles": true,
          "testPush": true,
          "prototype.updatePushToken": true,
          "prototype.updatePushTokenMobile": true,
          "prototype.changeProfileImage": true,
          "prototype.UpdateProfile": true,
          "loginBySocialMedia": true,
          "VerifyEmail": true,
          "RegisterUser": true
        }
      }
    }
  },
  "UsuarioAccessToken": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Upload": {
    "dataSource": "localFiles",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "download": true
        }
      }
    }
  },
  "UploadedFiles": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Notification": {
    "dataSource": "mysqlDev",
    "public": true
  },
  "PushTokens": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "Mail": {
    "dataSource": "mailDev",
    "public": false
  },
  "PasswordResetPIN": {
    "dataSource": "mysqlDev",
    "public": true
  },
  "adminMail": {
    "dataSource": "adminMail",
    "public": false
  },
  "UserPermissionPolicyNode": {
    "dataSource": "mysqlDev",
    "public": true
  },
  "UserPermissionPolicyRelation": {
    "dataSource": "mysqlDev",
    "public": false
  },
  "OrdersHistory": {
    "dataSource": "mysqlDev",
    "public": true
  },
  "UserData": {
    "dataSource": "mysqlDev",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  },
  "Teacher": {
    "dataSource": "mysqlDev",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "AddTeachers": true
        }
      }
    }
  },
  "Student": {
    "dataSource": "mysqlDev",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "AddStudents": true,
          "GetAllOfSchool": true
        }
      }
    }
  },
  "Subject": {
    "dataSource": "mysqlDev",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "GetAll": true
        }
      }
    }
  },
  "TeacherSubject": {
    "dataSource": "mysqlDev",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    }
  }
}
